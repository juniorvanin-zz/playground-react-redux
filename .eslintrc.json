{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
       "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
       "semi": ["warn", "never"],
       "no-use-before-define": "off",
       "no-underscore-dangle": "off",
       "comma-dangle": ["error", "never"],
       "react/forbid-prop-types": "off",
       "react/require-default-props": "off",
       "react/prop-types": "off"
    },
    "globals": {
      "document": true,
      "window": true
    }
}
